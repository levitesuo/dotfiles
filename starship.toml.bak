# Get editor completions based on the config schema

palette = "catppuccin_mocha"

continuation_prompt = '▶▶ '
add_newline = false

format = """
[](#f5c2e7)\
$os\
$time\
[](#f5c2e7 bg:#f9e2af)\
$directory\
[](fg:#f9e2af bg:#a6e3a1)\
[\
$c\
$elixir\
$elm\
$golang\
$gradle\
$haskell\
$java\
$julia\
$nim\
$rust\
$scala\
$python\
$lua\
$nodejs\
](bg:#a6e3a1 fg:black bold)\
[](fg:#a6e3a1 bg:#89b4fa)\
[\
$git_branch\
](bg:#89b4fa fg:black bold)\
[](fg:#89b4fa bg:#f38ba8)\
[$git_status](bg:#f38ba8 fg:black bold)\
[](fg:#f38ba8)
$character
"""

[character]
success_symbol = "    [└────❯](bold #93BEDF)"
error_symbol = "[    └────❯](bold #EF476F)"

[os]
disabled = false
style = "bg:#f5c2e7 fg:black bold"

[time]
style= "bg:#f5c2e7 fg:black bold"
disabled = false
format = '[$time ]($style)'
time_format = '%H:%M'
utc_time_offset = '+2'

[directory]
style = "fg:black bold bg:#f9e2af"
truncation_symbol = "󰜴/"
format = "[  $path $read_only]($style)"
read_only = " 󰌾"

[git_status]
format='$all_status( $ahead_behind)'
ahead = '${count}'
diverged = '${ahead_count}${behind_count}'
behind = '${count}'

[aws]
symbol = "  "

[buf]
symbol = " "

[c]
symbol = " "
style = "bf:prev_bg fg:black bold"

[cmake]
symbol = " "

[conda]
symbol = " "

[crystal]
symbol = " "

[dart]
symbol = " "

[docker_context]
symbol = " "

[elixir]
